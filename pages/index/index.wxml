    <view class='login-mask animated fadeIn masks'>
        <view class='login-wrap'>
            <i id='login-close' class="iconfont icon-guanbi close"></i>
            <view class='login-title'>输入手机号码</view>
            <!-- <input class='input-password' placeholder="这是一个可以自动聚焦的input" /> -->
            <view class="sixDigitPassword-box">
                <input class='sixInput' maxlength="4" />
                <i style="width: 24%; border-color: transparent;" class="">
                <b>1</b>
                </i>
                <i style="width: 24%;"><b>1</b></i>
                <i style="width: 24%;"><b>2</b></i>
                <i style="width: 24%;"><b>3</b></i>
            </view>
        </view>
    </view>
    <!-- Contenedor 常见问题question -->
    <ul id="accordion" class="accordion"  style="display: {{accordionFlag==1?'block':'none'}}">
        <view class='header'>
            <text class='close' bindtap='accordion'>关闭</text>
            <view class='title'>常见问题</view>
        </view>
        <li data-index='1' bindtap="toggle" class="{{active==1?'open':''}}">
            <div class="link">问题一<i class="iconfont icon-chevron-copy-copy-copy-copy-copy fa-chevron-down"></i></div>
            <ul class="submenu" wx:if="{{active=='1'}}">
                <li><a href="#">Photoshop</a></li>
                <li><a href="#">HTML</a></li>
                <li><a href="#">CSS</a></li>
                <li><a href="#">Maquetacion web</a></li>
            </ul>
        </li>
        <li data-index='2' bindtap="toggle" class="{{active==2?'open':''}}">
            <div class="link">问题二<i class="iconfont icon-chevron-copy-copy-copy-copy-copy fa-chevron-down"></i></div>
            <ul class="submenu" wx:if="{{active=='2'}}">
                <li><a href="#">Javascript</a></li>
                <li><a href="#">jQuery</a></li>
                <li><a href="#">Frameworks javascript</a></li>
            </ul>
        </li>
        <li data-index='3' bindtap="toggle" class="{{active==3?'open':''}}">
            <div class="link">问题三<i class="iconfont icon-chevron-copy-copy-copy-copy-copy fa-chevron-down"></i></div>
            <ul class="submenu" wx:if="{{active=='3'}}">
                <li><a href="#">Tablets</a></li>
                <li><a href="#">Dispositivos mobiles</a></li>
                <li><a href="#">Medios de escritorio</a></li>
                <li><a href="#">Otros dispositivos</a></li>
            </ul>
        </li>
        <li data-index='4' bindtap="toggle" class="{{active==4?'open':''}}">
            <div class="link">问题四<i class="iconfont icon-chevron-copy-copy-copy-copy-copy fa-chevron-down"></i></div>
            <ul class="submenu" wx:if="{{active=='4'}}">
                <li><a href="#">Google</a></li>
                <li><a href="#">Bing</a></li>
                <li><a href="#">Yahoo</a></li>
                <li><a href="#">Otros buscadores</a></li>
            </ul>
        </li>
        <view class='footer'>
            <text class='question'>问题还没有解决?</text>
            <button class='ft-btn' type="primary" size='100' bindtap='calling'>联系客服 </button>
        </view>
    </ul>
    <!-- 服务选择蒙层 -->
    <view  class="menu-mask masks animated {{menuFlag==1?'fadeInDown':'fadeOutUp'}}">
        <view class='menu-title'>服务选择</view>
        <i id='menu-mask-close' class="iconfont icon-guanbi close" bindtap="menuClick"></i>
        <view class='info-mask-ul menu-mask-ul' id='menu-mask-ul'>

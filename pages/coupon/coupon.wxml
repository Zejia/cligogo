    <view  wx:for="{{logs}}" wx:for-item='log' class='schedule-list {{log.s>-1 ? "" : "gray"}}'>
        <view class='schedule-header'>
            <view class='schedule-title'>
                <view class='big-title'>{{log.name}}</view>
                <view class='sec-title'>有效期 : {{log.createtime}}</view>
            </view>
            <view class='schedule-price'>
                    <text wx:if="{{log.s > -1}}">剩余</text> 
                    <text wx:else> 消费 </text>
                <text class="schedule-price-info">{{time[index]}}</text>
            </view>
        </view>
        <view class='schedule-footer'>
            <view class='schedule-title'>
                <!-- <view class='gray sec-title'>所有城市适用</view> -->
            </view>
        </view>
    </view>

page{
    background: #f3f4f5;
}
.schedule-list{
    background: #fff;
    width: 90%;
    margin: 60rpx  auto;
    border-bottom: 8rpx solid #eb8118;
    color: #eb8118;
    border-radius: 10rpx;
    -webkit-box-shadow: 0px 6px 9px -4px rgba(0,0,0,0.6);
    -moz-box-shadow: 0px 6px 9px -4px rgba(0,0,0,0.6);
    box-shadow: 0px 6px 9px -4px rgba(0,0,0,0.6);
}
.schedule-header{
    padding: 20rpx;
    margin: 20rpx;
    display: flex;
    justify-content: space-between;
}
.schedule-footer{
    padding: 0 20rpx;
    margin: 0 20rpx;
    margin-bottom: 10rpx;
    text-align: center;
    border:1px solid #eb8118;
    padding: 10rpx;
    border-radius: 10rpx;
    background:#3fb84c;
    color: #fff;
}
.big-title{
    font-size: 38rpx;
    margin-bottom: 10rpx;
    color:#000;
    font-weight:bold;

}
.sec-title{
    font-size: 28rpx;
    margin-bottom: 10rpx;

}
.schedule-price{
    font-size: 35rpx;
    border:1px solid #eb8118;
    border-radius: 5rpx;
    height: 70rpx;
    line-height: 70rpx;
    padding:0 20rpx;
    position: relative;
}
.schedule-price-info{
    font-size:30rpx;
    position:absolute;
    top:-34rpx;
    background:#fff;
    border:1px solid #eb8118;
    border-radius:30rpx;
    padding:8rpx 30rpx;
    height:31rpx;
    line-height:30rpx;
    
}
.gray{
    color: #a4a4a4;
}
.none-data{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    font-size: 30rpx;
    color: #a4a4a4;
    width: 100%;
    text-align: center;
}
.tips-share{
    margin-top: 20rpx;
    text-decoration: underline;
    color: #ff0000;
}
.tips{
    font-size: 30rpx;
    color: #a4a4a4;
    width: 90%;
    text-align: left;
    margin: 40rpx;
}
.line-through{
    text-decoration: line-through;
}


.red_color{
    border:1px solid #ff0000;
    background: #ff0000;
    color: #fff;
}
.black_color{
    color:#3fb84c;
}
.black_border_color{
    border: 1px solid #3fb84c;
    
}
.black_border_bottom_color{
    border-bottom: 0rpx solid #3fb84c;
}

.blue_color{
    color: rgba(5, 113, 255, 0.82);
}
.blue_border_color{
    border: 1px solid rgba(5, 113, 255, 0.82);
}
.blue_border_bottom_color{
    border-bottom: 8px solid rgba(5, 113, 255, 0.82);
}

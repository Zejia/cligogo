/* 新增样式 shopping-card*/
.textures{
    background: url(http://cdnimg.bama77.com/product-bg.png) top no-repeat;
    background-size: 100% 106%;
}
.textures-box {
    margin-left: 40rpx;
    padding-left: 10rpx;
    padding-right: 44rpx;
}
.title-h2{
    font-size: 30rpx;
    color: #262626;
    margin: 0;
    padding-bottom: 12rpx;
    padding-top: 60rpx;
}
.price-box{
    border-bottom: 1px solid #f5f5f5;
    padding-bottom: 8px;
}
.title-h3{
    font-size: 28rpx;
    margin: 0;
}
.price-red{
    color: #ff4891;
    font-size: 26rpx;
    line-height: 40rpx;
}
.price-red .price-product {
    padding-left:10rpx;
    font-size: 46rpx;
}
.sale-num {
    color: #969696;
    font-size: 24rpx;
    float: right;
    margin-top:7rpx;
    
}
.product-news{
    display:flex;
    align-items: flex-end;
    padding-bottom:20rpx;
    padding-top:10rpx;
}
.product-model{
    width: 33%;
    font-size: 24rpx;
}
.product-model image {
    width: 36rpx;
    line-height: 44px;
    margin-right: 10rpx;
}
.binding{
    line-height:52rpx;
    color: #969696;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    -ms-text-overflow: ellipsis;
    text-overflow: ellipsis;
    text-align: left;
    display: inline-block;
    vertical-align: bottom;
}

/* 亮点 */

.lights-pots{
    background: #fff;
    padding: 12rpx;
    border-top: 20rpx solid #f5f5f5;

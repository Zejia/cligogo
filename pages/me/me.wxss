.header{
    border-bottom: 1rpx solid #e5e5e5;
    text-align: center;
    margin:0 auto;
}
.header-img{
    background:url(https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1503288743056&di=486431acf9ab2f8c0f0134fa2764742e&imgtype=0&src=http%3A%2F%2Fimg3.duitang.com%2Fuploads%2Fitem%2F201508%2F01%2F20150801104534_LS25k.thumb.700_0.png) no-repeat center center;
    background-size: cover;
    width: 160rpx;
    height: 160rpx;
    margin: 20rpx;
    border-radius:80rpx;
    border: 1rpx solid #f4f4f4;
    margin:0 auto;
}
.header-title{
    margin-left: 10rpx;
    font-size: 34rpx;
    color: #d4d4d4;
    margin:10rpx;
}

.setting-list{
    color: #1f1f1f;
    font-size: 34rpx;
    height: 80rpx;
    line-height: 80rpx;
}
.setting-list view{
    position: relative;
    padding:20rpx;
    border-bottom: 2rpx solid #e5e5e5;

}
.setting-list view::after{
    content:">";
    position: absolute;
    right: 30rpx;
    top: 40rpx;
    font-size: 34rpx;
    line-height: 34rpx;
}
button[type='warn']{
 border-radius: 0;
}
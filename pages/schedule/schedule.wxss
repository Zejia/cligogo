page{
    background: #f3f4f5;
}
.schedule-list{
    background: #fff;
    width: 90%;
    margin: 0 auto;
    border-bottom: 8rpx solid #3fb84c;
    color: #3fb84c;
    border-radius: 10rpx;
    -webkit-box-shadow: 0px 5px 21px 0px rgba(222,219,222,1);
    -moz-box-shadow: 0px 5px 21px 0px rgba(222,219,222,1);
    box-shadow: 0px 5px 21px 0px rgba(222,219,222,1);
}
.schedule-header{
    padding: 20rpx;
    margin: 40rpx 20rpx;
    display: flex;
    justify-content: space-between;
}
.schedule-footer{
    padding: 0 20rpx;
    margin: 0 20rpx;
    margin-bottom: 10rpx;
    display: flex;
    justify-content: space-between;
}
.big-title{
    font-size: 45rpx;
    margin-bottom: 10rpx;
}
.sec-title{
    font-size: 30rpx;
    margin-bottom: 10rpx;

}
.schedule-price{
    font-size: 40rpx;
}
.schedule-price-info{
    font-size: 60rpx;
}
.gray{
    color: #a4a4a4;
}

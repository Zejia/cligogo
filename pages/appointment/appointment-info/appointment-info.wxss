}


.div_center {
    width: 50%;
    position: relative;
    margin-top: 7px;
    margin-left: 2%;
}

.div_right {
    border: solid 1px #e6e6e6;
    border-radius: 4px;
    width: 82px;
    overflow: hidden;
    text-align: center;
    line-height: 30px;
    box-sizing: border-box;
}

.div_right i {
    float: left;
    width: 32%;
    color: #333;
}

.div_right span {
    float: left;
    width: 33%;
    border-left: solid 1px #e6e6e6;
    border-right: solid 1px #e6e6e6;
    color: #333;
}

.container {
    background: #fff;
}
/*整体列表*/

.carts-list {
    display: flex;
    flex-direction: column;
    padding: 20rpx 40rpx;
}
/*每行单元格*/

.carts-item {
    display: flex;
    flex-direction: row;
    height: 150rpx;
    /*width属性解决标题文字太短而缩略图偏移*/
    width: 100%;
    border-bottom: 1px solid #eee;
    padding: 30rpx 0;
    position:relative;

}
/*左部图片*/

.carts-image {
    width: 150rpx;
    height: 150rpx;
}
/*右部描述*/

.carts-text {
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
/*右上部分标题*/

.carts-title {
    margin: 10rpx;
    font-size: 30rpx;
}
/*右下部分价格与数量*/

.carts-subtitle {
    font-size: 25rpx;
    color: darkgray;
    padding: 0 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
/*价格*/

.carts-price {
    color: #f60;
}
.tips{
    font-size: 28rpx;
}
.paynow{
    float: right;
    color: blue;
    text-decoration: underline;
}